##
var a:= new integer[5](1,2,3,4,5);
Println(a);
var x:= ReadlnInteger;
////
if x=1 then 
  begin
for var i:=0 to 3 do
  a[i]:=a[i+1];
end;
////
if x=2 then 
  begin
for var i:=1 to 3 do
  a[i]:=a[i+1];
end;
////
if x=3 then 
  begin
for var i:=2 to 3 do
  a[i]:=a[i+1];
end;
////
if x=4 then 
  begin
for var i:=3 to 3 do
  a[i]:=a[i+1];
end;
////
if x=5 then 
  begin
for var i:=3 to 4 do
  SetLength(a,4);
end;
////
SetLength(a,4);
Println(a)
